# tm-scan configuration file
# Copy this to .env and fill in your values

# GitHub Personal Access Token (get from https://github.com/settings/tokens)
GITHUB_TOKEN=ghp_your_token_here

# Alternative token environment variable name
# TM_GITHUB_TOKEN=ghp_your_token_here

# Default organization (optional, can override with --org)
# TM_ORG=mbbgrp

# Default output directory (optional)
# TM_OUTPUT_DIR=~/tm-output

# Default workspace directory (optional)
# TM_WORKSPACE_DIR=~/tm-workspace
